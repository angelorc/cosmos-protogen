{
  "name": "cosmos-protogen",
  "version": "0.0.0",
  "description": "",
  "repository": "angelorc/cosmos-protogen",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "cosmos-protogen": "./dist/cli.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "vitest dev",
    "cosmos-protogen": "jiti ./src/cli.ts",
    "lint": "eslint . && prettier -c .",
    "lint:fix": "automd && eslint . --fix && prettier -w .",
    "prepack": "pnpm build",
    "play": "jiti playground",
    "release": "pnpm test && changelogen --release && npm publish && git push --follow-tags",
    "test": "pnpm lint && pnpm test:types && vitest run --coverage",
    "test:types": "tsc --noEmit --skipLibCheck"
  },
  "devDependencies": {
    "@types/node": "^22.1.0",
    "@vitest/coverage-v8": "^2.0.5",
    "automd": "^0.3.8",
    "changelogen": "^0.5.5",
    "consola": "^3.2.3",
    "eslint": "^9.8.0",
    "eslint-config-unjs": "^0.3.2",
    "jiti": "^2.0.0-beta.3",
    "ofetch": "^1.4.1",
    "pathe": "^1.1.2",
    "prettier": "^3.3.3",
    "protobufjs": "^7.4.0",
    "tar": "^7.4.3",
    "ts-poet": "^6.9.0",
    "ts-proto": "^2.2.4",
    "typescript": "^5.5.4",
    "unbuild": "^3.0.0-rc.7",
    "vitest": "^2.0.5"
  },
  "packageManager": "pnpm@9.7.0",
  "dependencies": {
    "citty": "^0.1.6"
  }
}